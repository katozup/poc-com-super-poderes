{
  "version": 2,
  "projects": {
    "itau-mgm-promoter-credicard": {
      "root": "apps/itau-mgm-promoter-credicard",
      "sourceRoot": "apps/itau-mgm-promoter-credicard/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/itau-mgm-promoter-credicard",
            "index": "apps/itau-mgm-promoter-credicard/src/index.html",
            "main": "apps/itau-mgm-promoter-credicard/src/index.js",
            "polyfills": "apps/itau-mgm-promoter-credicard/src/polyfills.ts",
            "tsConfig": "apps/itau-mgm-promoter-credicard/tsconfig.app.json",
            "assets": [
              "apps/itau-mgm-promoter-credicard/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/itau-mgm-promoter-credicard/src/environments/environment.ts",
                  "with": "apps/itau-mgm-promoter-credicard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "apps/promoter-credicard/src/environments/environment.ts",
                  "with": "apps/promoter-credicard/src/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "itau-mgm-promoter-credicard:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "itau-mgm-promoter-credicard:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/itau-mgm-promoter-credicard/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/itau-mgm-promoter-credicard"
          ],
          "options": {
            "jestConfig": "apps/itau-mgm-promoter-credicard/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "itau-mgm-promoter-credicard-e2e": {
      "root": "apps/itau-mgm-promoter-credicard-e2e",
      "sourceRoot": "apps/itau-mgm-promoter-credicard-e2e/src",
      "projectType": "application",
